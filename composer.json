{
    "name": "games647/minecraft-banner-generator",
    "keywords": ["minecraft", "banner", "server", "player", "online"],
    "description": "Customizable banner generator for servers and player",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/games647/Minecraft-Banner-Generator/",
    "support": {
        "issues": "https://github.com/games647/Minecraft-Banner-Generator/issues",
        "source": "https://github.com/games647/Minecraft-Banner-Generator"
    },
    "require": {
        "php": ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "games647/minecraft-skin-renderer": "^0.2.1",
        "xpaw/php-minecraft-query": "^1.0",
        "funnyitselmo/minecraftserverstatus": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MinecraftBanner\\": "src"
        }
    },
    "suggest": {
        "games647/minecraft-skin-renderer": "Allows you to render minecraft skin heads and complete skins for player banners",
        "xpaw/php-minecraft-query": "Allows you to query data from minecraft servers like: motd, online players, max players, favicon, ping",
        "funnyitselmo/minecraftserverstatus": "An alternative to xpaw's project"
    }
}
